generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Page {
  id        String   @id @default(cuid())
  slug      String   @unique
  content   Json[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id       String  @id @default(uuid())
  name     String?
  email    String  @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
